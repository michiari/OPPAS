program:
bool modified;
u8 vsize, vused;
u8[6] v;

main() {
  u8 a;
  modified = false;

  Stack::Stack();

  a = *;
  Stack::push(a);

  modified = false;
  Stack::pop();

  modified = false;
  Stack::push(42u8);

  modified = false;
  // T::Tcp();

  Stack::~Stack();
  T::~T();
}

StackImpl::StackImpl(u8 s) {
  _::operator_new();
  vsize = s;
  vused = 0u8;
}

StackImpl::~StackImpl() {
  std::destroy(0u8, vused);
  _::operator_delete();
}

StackImpl::swap() {
  std::swap();
  std::swap();
  std::swap();
}

Stack::Stack() {
  StackImpl::StackImpl(2u8);
}

Stack::Stackcp() {
  u8 i, x;
  StackImpl::StackImpl(vsize);
  i = 0u8;
  while (i < vused) {
    std::construct(v[i], x);
    v[i] = x;
    i = i + 1u8;
  }
}

Stack::operator=() {
  Stack::Stackcp();
  StackImpl::swap();
}

Stack::size() {}

Stack::push(u8 t) {
  u8 i, oldvused;
  u8[6] oldv;
  if (vused == vsize) {
    i = 0u8;
    while (i < vused) {
      oldv[i] = v[i];
      i = i + 1u8;
    }
    oldvused = vused;
    StackImpl::StackImpl(vsize + 2u8);
    while (vused < oldvused) {
      Stack::push(oldv[vused]);
      vused = vused + 1u8;
    }
    Stack::push(t);
    StackImpl::swap();
  } else {
    std::construct(t, i);
    v[vused] = i;
    modified = true;
    vused = vused + 1u8;
  }
}

Stack::top(u8 &x) {
  T::Tcp(v[vused], x);
}

Stack::pop() {
  if (vused == 0u8) {
    throw;
  } else {
    std::destroy(vused - 1u8, vused);
    vused = vused - 1u8;
    modified = true;
  }
}

Stack::~Stack() {
  StackImpl::~StackImpl();
}

T::T(u8 &val) {
  if (*) {
    throw;
  } else {
    val = 0u8;
  }
}

T::Tcp(u8 src, u8 &dst) {
  T::operator=(src, dst);
}

T::operator=(u8 src, u8 &dst) {
  if (*) {
    throw;
  } else {
    dst = src;
  }
}

T::~T() {}

std::swap() {
  modified = true;
}

std::construct(u8 src, u8 &dst) {
  T::Tcp(src, dst);
}

std::destroy(u8 from, u8 to) {
  while (from < to) {
    T::~T();
    from = from + 1u8;
  }
}

