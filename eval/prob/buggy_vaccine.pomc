probabilistic query: approximate;

program:

main() {
    query young();
}

young() {
  bool get_vaccine;
  u2 y, e;
  u4 sum;

  y = 0u2 {1u3 : 3u3} 1u2 {1u3 : 3u3} 2u2;
  e = 0u2 {1u3 : 2u3} 1u2;
  get_vaccine = 0u2 {1u2 : 2u2} 1u2;
  sum = y + e;
  

  while (y > 0u2) {
    query young();
    y = y - 1u2;
  }

  while (e > 0u2) {
    query elder();
    e = e - 1u2;
  }

  observe !get_vaccine || sum < 2u4;
}

elder() {
  bool get_vaccine;
  u3 y, e;

  y = 0u3 {1u2 : 2u2} 1u3;
  e = 0u3 {1u3 : 4u3} 1u3 {1u3 : 4u3} 2u3 {1u3 : 4u3} 3u3;
  get_vaccine = 0u2 {1u2 : 2u3} 1u2;
  observe !get_vaccine || y + e < 2u4;

  while (y > 0u3) {
    query young();
    y = y - 1u3;
  }

  while (e > 0u3) {
    query elder();
    e = e - 1u3;
  }

}
