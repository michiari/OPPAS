probabilistic query: approximate;
formula = G ((call And alice And isFair) -> ~ (XNu obs));

program:
u3 fairness;
main() {
  bool res, isFair;
  fairness = 0u3 {1u3 : 4u3} 1u3 {1u3 : 4u3} 2u3 {1u3 : 4u3} 3u3;
  isFair = fairness == 3u3;
  query alice(res);
  // res is which caf√® they have gone to
 
}

alice(bool &x) {
    bool prior_alice, bob_choice;
    // sample according to the prior (0.8)
    prior_alice = 1u1 {8u4 : 10u4} 0u1;
    query bob(bob_choice);
    observe prior_alice == bob_choice;
    x = prior_alice;
}

bob(bool &y) {
    bool prior_bob, recurse, alice_choice;
    // sample according to the prior (0.5)
    prior_bob = 1u1 {4u4 : 10u4} 0u1;
    recurse = 1u1 {3u3 : 5u3} 0u1;
    if (recurse) {
        query alice(alice_choice);
        observe prior_bob == alice_choice;
    } else {}
    y = prior_bob;
}
